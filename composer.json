{
    "name": "catgento/module-admin-activity",
    "description": "The Admin Activity extension makes it easy to track all admin activity and revert the modification.",
    "type": "magento2-module",
    "version": "1.1.5",
    "license": [
        "OSL-3.0"
    ],
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Kiwi Commerce Ltd",
            "email": "hello@kiwicommerce.co.uk",
            "homepage": "https://kiwicommerce.co.uk/",
            "role": "Leader"
        },
        {
            "name": "catgento",
            "homepage": "https://www.catgento.com",
            "role": "Forker"
        }
    ],
    "require": {
        "php": "^8.2 || ^8.3",
        "magento/framework": "^103.0",
        "magento/module-backend": "^103.0"
    },
    "require-dev": {
        "magento/magento-coding-standard": "^35",
        "phpstan/phpstan": "^1.11",
        "friendsofphp/php-cs-fixer": "^3.64",
        "rector/rector": "^1.0"
    },
    "config": {
      "allow-plugins": {
        "dealerdirect/phpcodesniffer-composer-installer": true
      },
      "sort-packages": true
    },
    "keywords": [
        "magento",
        "admin",
        "backend",
        "activity",
        "log"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Catgento\\AdminActivity\\": ""
        }
    }
}
